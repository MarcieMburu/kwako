<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PostProperty">

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="House Type"
        android:textAllCaps="false"
        android:textAppearance="@style/heading_2"
        android:textFontWeight="500"
        app:layout_constraintBottom_toTopOf="@+id/edtHouseType"
        app:layout_constraintEnd_toEndOf="@+id/edtHouseType"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edtHouseType" />

    <EditText
        android:id="@+id/edtPhone"
        android:layout_width="292dp"
        android:layout_height="50dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/frame_9"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtHouseType" />

    <ImageView
        android:id="@+id/previousImageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_arrow_back_ios_24" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:text="Enter Property Details"
        android:textAppearance="@style/heading_2"
        android:textFontWeight="700"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.334"
        app:layout_constraintStart_toEndOf="@+id/previousImageView"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Location"
        android:textAllCaps="false"
        android:textAppearance="@style/heading_2"
        android:textFontWeight="500"
        app:layout_constraintBottom_toTopOf="@+id/edtLocation"
        app:layout_constraintEnd_toEndOf="@+id/edtLocation"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edtLocation" />

    <EditText
        android:id="@+id/edtLocation"
        android:layout_width="292dp"
        android:layout_height="50dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/frame_9"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Price"
        android:textAllCaps="false"
        android:textAppearance="@style/heading_2"
        android:textFontWeight="500"
        app:layout_constraintBottom_toTopOf="@+id/edtPrice"
        app:layout_constraintEnd_toEndOf="@+id/edtPrice"
        app:layout_constraintHorizontal_bias="0.003"
        app:layout_constraintStart_toStartOf="@+id/edtPrice" />

    <EditText
        android:id="@+id/edtPrice"
        android:layout_width="292dp"
        android:layout_height="50dp"
        android:layout_marginTop="44dp"
        android:background="@drawable/frame_9"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtLocation" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Phone Number"
        android:textAllCaps="false"
        android:textAppearance="@style/heading_2"
        android:textFontWeight="500"
        app:layout_constraintBottom_toTopOf="@+id/edtPhone"
        app:layout_constraintEnd_toEndOf="@+id/edtPhone"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edtPhone" />

    <EditText
        android:id="@+id/edtHouseType"
        android:layout_width="292dp"
        android:layout_height="50dp"
        android:layout_marginTop="44dp"
        android:background="@drawable/frame_9"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtPrice" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WhatsApp Number"
        android:textAllCaps="false"
        android:textAppearance="@style/heading_2"
        android:textFontWeight="500"
        app:layout_constraintBottom_toTopOf="@+id/edtWhatsAppNo"
        app:layout_constraintEnd_toEndOf="@+id/edtWhatsAppNo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edtWhatsAppNo" />

    <EditText
        android:id="@+id/edtWhatsAppNo"
        android:layout_width="292dp"
        android:layout_height="50dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/frame_9"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtPhone" />

    <Button
        android:id="@+id/nextbtn"
        android:layout_width="292dp"
        android:layout_height="50dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/frame_14"
        android:text="Next"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtWhatsAppNo" />

</androidx.constraintlayout.widget.ConstraintLayout>